        <!-- table QUALITY_ANNOTATION_SENSOR -->
        <addForeignKeyConstraint
            constraintName="FK_QUALITY_ANNOTATION_SENSOR_ANNOTATION_ID"
            baseTableName="QUALITY_ANNOTATION_SENSOR" baseColumnNames="ANNOTATION_ID"
            referencedTableName="QUALITY_ANNOTATION" referencedColumnNames="ID"
            deferrable="false" initiallyDeferred="false"
            onDelete="CASCADE" onUpdate="CASCADE"/>
        <addForeignKeyConstraint
            constraintName="FK_QUALITY_ANNOTATION_SENSOR_SENSOR_ID"
            baseTableName="QUALITY_ANNOTATION_SENSOR" baseColumnNames="SENSOR_ID"
