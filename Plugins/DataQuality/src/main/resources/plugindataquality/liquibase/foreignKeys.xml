    <changeSet author="scf" id="2021-01-01-multiDatastreams-foreignKeys" objectQuotingStrategy="QUOTE_ALL_OBJECTS">
        <preConditions onFail="MARK_RAN">
            <not>
                <foreignKeyConstraintExists foreignKeyName="FK_QUALITY_ANNOTATION_SCHEME_ID" foreignKeyTableName="QUALITY_ANNOTATION" />
            </not>
        </preConditions>

        <!-- table QUALITY_ANNOTATION -->
        <addForeignKeyConstraint
            constraintName="FK_QUALITY_ANNOTATION_SCHEME_ID"
            baseTableName="QUALITY_ANNOTATION" baseColumnNames="SCHEME_ID"
            referencedTableName="QUALITY_SCHEME" referencedColumnNames="ID"
            deferrable="false" initiallyDeferred="false"
            onDelete="CASCADE" onUpdate="CASCADE"/>
        <addForeignKeyConstraint
            constraintName="FK_QUALITY_ANNOTATION_SETUP_ID"
            baseTableName="QUALITY_ANNOTATION" baseColumnNames="SETUP_ID"
            referencedTableName="QUALITY_SETUP" referencedColumnNames="ID"
            deferrable="false" initiallyDeferred="false"
            onDelete="CASCADE" onUpdate="CASCADE"/>
        <addForeignKeyConstraint
            constraintName="FK_QUALITY_ANNOTATION_MEASURE_ID"
            baseTableName="QUALITY_ANNOTATION" baseColumnNames="MEASURE_ID"
            referencedTableName="QUALITY_MEASURE" referencedColumnNames="ID"
            deferrable="false" initiallyDeferred="false"
            onDelete="CASCADE" onUpdate="CASCADE"/>

        <!-- table QUALITY_ANNOTATION_HIST -->
        <addForeignKeyConstraint
            constraintName="FK_QUALITY_ANNOTATION_HIST_ANNOTATION_ID"
            baseTableName="QUALITY_ANNOTATION_HIST" baseColumnNames="ANNOTATION_ID"
            referencedTableName="QUALITY_ANNOTATION" referencedColumnNames="ID"
            deferrable="false" initiallyDeferred="false"
            onDelete="CASCADE" onUpdate="CASCADE"/>
        <addForeignKeyConstraint
            constraintName="FK_QUALITY_ANNOTATION_HIST_OBSERVATION_ID"
            baseTableName="QUALITY_ANNOTATION_HIST" baseColumnNames="OBSERVATION_ID"
            referencedTableName="OBSERVATIONS" referencedColumnNames="ID"
            deferrable="false" initiallyDeferred="false"
            onDelete="CASCADE" onUpdate="CASCADE"/>
        <addForeignKeyConstraint
            constraintName="FK_QUALITY_ANNOTATION_HIST_SENSOR_ID"
            baseTableName="QUALITY_ANNOTATION_HIST" baseColumnNames="SENSOR_ID"
            referencedTableName="SENSORS" referencedColumnNames="ID"
            deferrable="false" initiallyDeferred="false"
            onDelete="CASCADE" onUpdate="CASCADE"/>

        <!-- table QUALITY_SETUP_MEASURE -->
        <addForeignKeyConstraint
            constraintName="FK_QUALITY_SETUP_MEASURE_SETUP_ID"
            baseTableName="QUALITY_SETUP_MEASURE" baseColumnNames="SETUP_ID"
