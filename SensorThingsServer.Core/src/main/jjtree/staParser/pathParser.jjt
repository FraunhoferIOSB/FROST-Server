/**
 * Properties.
 */
void pId(): {}
{
  <EP_ID>
  (<PATH_SEPARATOR> ppValue() )?
}

void pSelfLink(): {}
{
  <EP_SELFLINK>
  (<PATH_SEPARATOR> ppValue() )?
}

void pDescription(): {}
{
  <EP_DESCRIPTION>
  (<PATH_SEPARATOR> ppValue() )?
}

void pDefinition(): {}
{
  <EP_DEFINITION>
  (<PATH_SEPARATOR> ppValue() )?
}

void pEncodingType(): {}
{
  <EP_ENCODINGTYPE>
  (<PATH_SEPARATOR> ppValue() )?
}

void pFeature(): {}
{
  <EP_FEATURE>
  (<PATH_SEPARATOR> ppValue() )?
}

void pLocation(): {}
{
  <EP_LOCATION>
  (<PATH_SEPARATOR> ppValue() )?
}

void pMetadata(): {}
{
