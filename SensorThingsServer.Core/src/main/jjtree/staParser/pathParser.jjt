  | pId()
  | pSelfLink()
  | pObservedArea()
  | pPhenomenonTime()
  | pResultTime()
  | pUnitOfMeasurements()
  | pName()
  | pDescription()
  | pObservationType()
  | pMultiObservationDataTypes()
  | cpRef()
  )
}


