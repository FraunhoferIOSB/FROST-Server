void pParameters(): {}
{
  <EP_PARAMETERS>
  (<PATH_SEPARATOR> (ppValue() | ppSubProperty()) )?
}

void pProperties(): {}
{
  <EP_PROPERTIES>
  (<PATH_SEPARATOR> (ppValue() | ppSubProperty()) )?
}

void pResult(): {}
{
  <EP_RESULT>
  (ppArrayIndex())?
  (<PATH_SEPARATOR> (ppValue() | ppSubProperty()) )?
}

